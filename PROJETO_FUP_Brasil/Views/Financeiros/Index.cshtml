@model IEnumerable<PROJETO_FUP_Brasil.Models.Financeiro>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Entrada
            </th>
            <th>
                Saida
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Aluno.Cursos.ValorCurso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Funcionario.SalarioFuncionario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FinanceiroId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FinanceiroId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FinanceiroId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
